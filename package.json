{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && workbox generateSW workbox-config.js"
    },
    "devDependencies": {
        "axios": "^0.19",
        "cross-env": "^5.2.1",
        "laravel-echo": "^1.6.1",
        "laravel-echo-server": "^1.6.0",
        "laravel-mix": "^4.1.4",
        "laravel-mix-bundle-analyzer": "^1.0.2",
        "laravel-mix-svelte": "^0.1.2",
        "lodash": "^4.17.15",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.23.7",
        "sass-loader": "^7.3.1",
        "socket.io-client": "^2.3.0",
        "svelte": "^3.16.0",
        "svelte-loader": "^2.13.6",
        "vue-template-compiler": "^2.6.10",
        "workbox-cli": "^4.3.1",
        "workbox-webpack-plugin": "^4.3.1"
    },
    "dependencies": {
        "bulma-toast": "^1.5.4"
    }
}
